{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "xo"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "linebreak-style": "off",
        "no-mixed-spaces-and-tabs": "off",
        "new-cap":"off",
        "no-useless-constructor": "off",
        "no-redeclare": "off",
        "no-unused-vars": "off"
    }
}
